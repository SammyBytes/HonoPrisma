{
  "name": "hono-prisma",
  "version": "0.0.2",
  "description": "Prisma ORM plugin for HonoJS framework",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc && bun build src/index.ts --outdir dist --target bun --external:hono --external:typescript --minify",
    "test": "bun test",
    "prepare": "bun run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "hono",
    "prisma",
    "middleware",
    "typescript",
    "bun"
  ],
  "author": "Samuel Luciano",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5",
    "hono": "^4.9.6",
    "@prisma/client": "^5"
  },
  "dependencies": {
    "hono": "^4.9.6"
  }
}
